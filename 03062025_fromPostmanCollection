      "post": {
        "tags": [
          "Firewall API > Firewalls"
        ],
        "summary": "Run action",
        "description": "Action you want to do to a firewall.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "action": "approveManagement"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "X-Tenant-ID",
